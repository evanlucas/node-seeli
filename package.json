{
  "name": "seeli",
  "version": "11.0.1",
  "description": "Object oriented, flexible CLI tools",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap",
    "tap": "tap",
    "lint": "eslint ./",
    "lint:fix": "npm run lint -- --fix",
    "check": "check-pkg",
    "release": "semantic-release"
  },
  "eslintConfig": {
    "root": true,
    "extends": "eslint-config-codedependant",
    "rules": {
      "no-eq-null": 0,
      "no-var": 2,
      "prefer-const": [
        "error"
      ],
      "no-cond-assign": 0
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "script"
    },
    "ignorePatterns": [
      "node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com/esatterwhite/node-seeli.git"
  },
  "keywords": [
    "CLI",
    "OOP",
    "command",
    "line",
    "class",
    "terminal",
    "ANSI",
    "cli",
    "opts",
    "nopt",
    "options",
    "args",
    "argv",
    "interactive"
  ],
  "author": {
    "name": "Eric Satterwhite",
    "email": "esatterwhite@wi.rr.com"
  },
  "contributors": [
    {
      "name": "Dennis Ploeger",
      "email": "develop@dieploegers.de",
      "url": "http://dennis.dieploegers.de"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "bugs": {
    "url": "https://github.com/esatterwhite/node-seeli/issues"
  },
  "homepage": "https://github.com/esatterwhite/node-seeli",
  "dependencies": {
    "abbrev": "^1.1.1",
    "chalk": "^4.0.0",
    "cliui": "^6.0.0",
    "clone": "^2.1.2",
    "debug": "^4.1.1",
    "inquirer": "^7.1.0",
    "mout": "^1.2.2",
    "nopt": "^4.0.3",
    "ora": "^4.0.3",
    "string-width": "^4.2.0",
    "strip-ansi": "^6.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "check-pkg": "^2.1.1",
    "cli-spinners": "^2.3.0",
    "eslint": "^7.9.0",
    "eslint-config-codedependant": "^1.0.0",
    "semantic-release": "^17.1.1",
    "tap": "^14.10.2",
    "vuepress": "^1.5.4"
  },
  "tap": {
    "jsx": false,
    "ts": false,
    "browser": false,
    "esm": false,
    "coverage-report": [
      "text",
      "text-summary",
      "json"
    ],
    "files": [
      "test/**/*.js"
    ],
    "nyc-arg": [
      "--exclude=test/",
      "--exclude=examples/"
    ]
  }
}
